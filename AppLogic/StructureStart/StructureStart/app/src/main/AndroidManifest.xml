<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.robin.Structure">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/triaup_launcher"
        android:label="TriangleApp"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name="com.example.robin.Structure.control.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.robin.Structure.view.RunActivity"
            android:label="@string/title_activity_run"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".view.OptionsActivity"
            android:label="@string/title_activity_options" />
        <activity android:name="com.example.robin.Structure.view.OptionsAcitvity" />
        <activity android:name="com.example.robin.Structure.view.StartActivity"></activity>
    </application>

</manifest>